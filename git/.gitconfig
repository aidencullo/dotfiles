[alias]
	A = add -A
	ac = !git A && git commit -m
	acp = !git add . && git ci 'N/A' && git h
	aal = config --global
	ali = ! echo git config --global alias
	al = config --get-regexp ^alias
	b = branch
	ba = branch -a
	ci = commit -m
	cl = clean -n
	cld = clean -nd
	cldf = clean -fd
	clf = clean -f
	cli = clean -ndx
	clif = clean -fdx
	clx = clean -nX
	clxd = clean -ndX
	clxf = clean -fX
	clxdf = clean -fdX
	co = checkout
	df = diff
	dfs = diff --stat
	f = fetch
	h = push origin HEAD
	last = log -1 HEAD
	lg = log --graph --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%an%C(reset)%C(bold yellow)%d%C(reset) %C(dim white)- %s%C(reset)' -n 10 --all
	p = push origin main
	rc = !git commit -am
	r = reset
	sh = show --stat
	st = status --ignored
	sta = stash -u

[core]
	editor = emacs
	excludesfile = /Users/aiden/.gitignore_global
	pager = 

[fetch]
	prune = true

[init]
	defaultBranch = main

[pager]
	branch = false

[push]
	default = current

[safe]
	directory = /home/runner/work/pipenv/pipenv

[tag]
	gpgsign = true

[user]
	email = aidencullo@gmail.com
	name = Aiden Cullo
