[user]
	name = Aiden Cullo
	email = aidencullo@gmail.com
[core]
	excludesfile = /Users/aiden/.gitignore_global
	editor = emacs
	pager = 
[fetch]
	prune = true
[init]
	defaultBranch = main
[alias]
	ac = !git A && git commit -m
	rc = !git commit -am
	A = add -A
	p = push origin main
	al = config --get-regexp ^alias
	h = push origin HEAD
	last = log -1 HEAD
	st = status --ignored
	co = checkout
	ci = commit -m
	lg = log --graph --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%an%C(reset)%C(bold yellow)%d%C(reset) %C(dim white)- %s%C(reset)' -n 10
	aal = config --global
	sh = show --stat
	df = diff
	dfs = diff --stat
	cl = clean -n
	r = reset
	clf = clean -f
	f = fetch
	ali = ! echo git config --global alias
	cli = clean -ndx
	clif = clean -fdx
	b = branch
	ba = branch -a
	cld = clean -nd
	cldf = clean -fd
	clx = clean -nX
	clxf = clean -fX
	clxd = clean -ndX
	clxdf = clean -fdX
	acp = !git add . && git ci 'N/A' && git h
	sta = stash -u
[push]
	default = current
[pager]
	branch = false
[safe]
	directory = /home/runner/work/pipenv/pipenv
	directory = /home/runner/work/pipenv/pipenv
