[user]
	name = Aiden Cullo
	email = aidencullo@gmail.com
[core]
	excludesfile = /Users/aiden/.gitignore_global
	editor = vim
[fetch]
	prune = true
[init]
	defaultBranch = main
[alias]
	ac = !git A && git commit -m
	rc = !git commit -am
	A = add -A
	p = push origin main
	al = config --get-regexp ^alias
	h = push origin HEAD
	last = log -1 HEAD
	st = status --ignored
	co = checkout
	ci = commit
	br = branch
	lg = log --graph --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%an%C(reset)%C(bold yellow)%d%C(reset) %C(dim white)- %s%C(reset)' --all
	aal = config --global
	sh = show --stat
	df = diff
	dfs = diff --stat
	cl = clean -xdn
	r = reset
	clf = clean -xdf
[push]
	default = current
